{
    "name": "jorge/fisioterapia-system",
    "description": "Sistema de gestiÃ³n de citas para fisioterapeuta",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.8",
        "google/apiclient": "^2.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
